ALGORITHM Dot roduct
VAR
   v1,v2 : ARRAY_OF INREGER[50];
   ps: INREGER; 

BEGIN
    function dotProduct(v1, v2){
    ps = 0;
    for(let i = 0; i < v1.length; i++){
        ps := ps + v1[i] * v2[i]
      }
    return ps
  }
  
  for(let i = 0; i < v1.length; i++){
    for(let j = i + 1; j < v2.length; j++){
      if(dotProduct(v1[i], v2[j]) === 0){
          Write ( "v1 et v2 sont orthogonaux")
      }
    }
  }
END



